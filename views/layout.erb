<html>
  <head>
    <title>Code coverage</title>
    <script src='./assets/jquery-1.4.2.min.js' type='text/javascript'></script>
    <script src='./assets/jquery.dataTables.min.js' type='text/javascript'></script>   
    <script src='./assets/fancybox/jquery.fancybox-1.3.1.pack.js' type='text/javascript'></script>   
    <script src='./assets/jquery.timeago.js' type='text/javascript'></script>   
    <script src='./assets/app.js' type='text/javascript'></script>    
    <link href='./assets/stylesheet.css' media='screen, projection, print' rel='stylesheet' type='text/css'>
    <link href='./assets/fancybox/jquery.fancybox-1.3.1.css' media='screen, projection, print' rel='stylesheet' type='text/css'>
    <link href='./assets/smoothness/jquery-ui-1.8.4.custom.css' media='screen, projection, print' rel='stylesheet' type='text/css'>    
  </head>
  
  <body>
    <div class="timestamp">Generated <%= timeago(Time.now) %></div>
    <ul class="group_tabs"></ul>

    <div id="content">
      <%= formatted_file_list("All Files", result.source_files) %>

      <% result.groups.each do |name, files| %>
        <%= formatted_file_list(name, files) %>
      <% end %>
    </div>
    
    <div id="footer">
      Generated by <a href="http://github.com/colszowka/simple_cov">SimpleCov</a>
    </div>
    
    <div style="display:none;">
    <% result.source_files.each do |source_file| %>
      <%= formatted_source_file(source_file) %>
    <% end %>
    </div>
  </body>
</html>
